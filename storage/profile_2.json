  "ingame_hotkeys": "-- Auto Buff Reuser (vBot compatible)\n-- by ChatGPT\n\nlocal ITEM_IDS = {\n  damage = 22525,\n  protect = 22522,\n  hp = 22519\n}\n\nlocal BUFF_DURATION = 930  -- 15 minut\nlocal RETRY_DELAY = 12        -- sekundy\n\nlocal buffs = {\n  damage = {lastActivated = 0, active = false, nextTry = 0},\n  protect = {lastActivated = 0, active = false, nextTry = 0},\n  hp = {lastActivated = 0, active = false, nextTry = 0}\n}\n\n-- Obs³uga wiadomoœci typu \"MonsterYell\" (mode 17)\nonTextMessage(function(mode, text)\n  if mode ~= 17 then return end\n\n  local msg = text:lower()\n\n  if msg:find(\"bônus de\") or msg:find(\"bonus de\") then\n    if msg:find(\"damage\") then\n      buffs.damage.lastActivated = os.time()\n      buffs.damage.active = true\n      print(\"[BUFF] Damage buff detected!\")\n    elseif msg:find(\"protect\") then\n      buffs.protect.lastActivated = os.time()\n      buffs.protect.active = true\n      print(\"[BUFF] Protect buff detected!\")\n    elseif msg:find(\"vida máxima\") or msg:find(\"vida maxima\") then\n      buffs.hp.lastActivated = os.time()\n      buffs.hp.active = true\n      print(\"[BUFF] Max HP buff detected!\")\n    end\n  end\nend)\n\n-- G³ówna pêtla\nmacro(1000, \"Auto Buff Reuser\", function()\n  local now = os.time()\n\n  for name, itemId in pairs(ITEM_IDS) do\n    local buff = buffs[name]\n    local timeSince = now - buff.lastActivated\n\n    if not buff.active or timeSince >= BUFF_DURATION then\n      -- Buff wygas³, próbuj aktywowaæ co RETRY_DELAY sekund\n      if now >= buff.nextTry then\n        use(itemId)\n        buff.nextTry = now + RETRY_DELAY\n        print(\"[BUFF] Trying to activate \" .. name .. \" buff (item: \" .. itemId .. \")...\")\n      end\n    end\n  end\nend)\n",
