  "ingame_hotkeys": "-- CONFIG\nlocal BUFF_DURATION = 15 * 60 -- 15 minut w sekundach\nlocal RETRY_DELAY = 12 -- odstêp miêdzy próbami w sekundach\n\nlocal ITEM_IDS = {\n  damage = 22525,\n  protect = 22522,\n  hp = 22519\n}\n\n-- BUFFY: damage, protect, hp\nlocal buffs = {\n  damage = {lastActivated = 0, nextTry = 0, active = false},\n  protect = {lastActivated = 0, nextTry = 0, active = false},\n  hp = {lastActivated = 0, nextTry = 0, active = false}\n}\n\n-- Makro do automatycznego u¿ywania buffów\nmacro(1000, \"Auto Buff Reuser\", function()\n  local now = os.time()\n\n  for name, itemId in pairs(ITEM_IDS) do\n    local buff = buffs[name]\n    local timeSince = now - buff.lastActivated\n\n    -- Jeœli buff wygas³ lub jeszcze nie by³ aktywowany\n    if not buff.active or timeSince >= BUFF_DURATION then\n      -- Czas na kolejn¹ próbê?\n      if now >= buff.nextTry then\n        use(itemId)\n        buff.nextTry = now + RETRY_DELAY\n      end\n    end\n  end\nend)\n\n-- Nas³uchiwanie wiadomoœci tekstowych (tylko MonsterYell = mode 17)\nonTextMessage(function(mode, text)\n  if mode ~= 17 then return end\n\n  local msg = text:lower()\n\n  -- Obs³uga komunikatów o aktywacji buffów\n  if msg:find(\"bônus de\") or msg:find(\"bonus de\") then\n    if msg:find(\"damage\") then\n      buffs.damage.lastActivated = os.time()\n      buffs.damage.active = true\n    elseif msg:find(\"protect\") then\n      buffs.protect.lastActivated = os.time()\n      buffs.protect.active = true\n    elseif msg:find(\"vida máxima\") or msg:find(\"vida maxima\") then\n      buffs.hp.lastActivated = os.time()\n      buffs.hp.active = true\n    end\n    return\n  end\n\n  -- Obs³uga komunikatów o cooldownie (exhaust)\n  local seconds = msg:match(\"exausto por (%d+) segundos\")\n  if seconds then\n    seconds = tonumber(seconds)\n    if msg:find(\"damage\") then\n      buffs.damage.nextTry = os.time() + seconds\n    elseif msg:find(\"protect\") then\n      buffs.protect.nextTry = os.time() + seconds\n    elseif msg:find(\"vida máxima\") or msg:find(\"vida maxima\") then\n      buffs.hp.nextTry = os.time() + seconds\n    end\n  end\nend)\n\n\nlocal defense_head = 11712\nlocal normal_head = 11455\nlocal head_percent = 80\nlocal mana_low_threshold = 10\nlocal mana_regen_stop = 30\nlocal manaRegen = false\n\nmacro(20, \"berecik\", function()\n  local hp = hppercent()\n  local mp = manapercent()\n  local headItem = getHead()\n  local currentHead = headItem and headItem.id or 0\n\n  if hp == nil or mp == nil then\n    return\n  end\n\n  -- Sprawdzenie regeneracji many\n  if manaRegen then\n    if mp >= mana_regen_stop then\n      manaRegen = false\n      if currentHead ~= defense_head then\n        moveToSlot(findItem(defense_head), SlotHead)\n      end\n      return\n    else\n      if currentHead ~= normal_head then\n        moveToSlot(findItem(normal_head), SlotHead)\n      end\n      return\n    end\n  end\n\n  -- Sprawdzenie HP\n  if hp <= head_percent then\n    if mp > mana_low_threshold then\n      if currentHead ~= defense_head then\n        moveToSlot(findItem(defense_head), SlotHead)\n      end\n    else\n      manaRegen = true\n      if currentHead ~= normal_head then\n        moveToSlot(findItem(normal_head), SlotHead)\n      end\n    end\n  else\n    if currentHead ~= normal_head then\n      moveToSlot(findItem(normal_head), SlotHead)\n    end\n  end\nend)\n",
